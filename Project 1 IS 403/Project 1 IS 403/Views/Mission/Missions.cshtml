
@{
    ViewBag.Title = "Missions";
}

<h2>Missions</h2>


@using (Html.BeginForm("missionType", "Mission", FormMethod.Get))
{
    <fieldset>
        <h4>Select Mission</h4>
        <h5>@Html.DropDownList("contentType")</h5>
    </fieldset>

    string sDDLValue = Request.Form["contentType"].ToString();

    @Html.ActionLink("Select", "MissionFAQ", "Mission", new { sMission = sDDLValue }, new { @class = "btn btn-sm btn-default"})
}


